<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>XrmControlsGetSrc</Title>
      <Shortcut>XrmControlsGetSrc</Shortcut>
      <Description> 5.0+ Returns the current URL being displayed in an IFRAME or web resource. 
Xrm.Page.getControl("controlname").getSrc();</Description>
      <Author>cangfeng</Author>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="false"></Literal>
      </Declarations>
      <Code Language="javascript"><![CDATA[ Xrm.Page.getControl("controlname").getSrc(); $end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
