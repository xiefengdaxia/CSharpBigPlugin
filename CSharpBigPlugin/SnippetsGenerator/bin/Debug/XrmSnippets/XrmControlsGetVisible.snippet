<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>XrmControlsGetVisible</Title>
      <Shortcut>XrmControlsGetVisible</Shortcut>
      <Description> 5.0+ Returns a value that indicates whether the control is currently visible. 
Xrm.Page.getControl("controlname").getVisible();</Description>
      <Author>cangfeng</Author>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="false"></Literal>
      </Declarations>
      <Code Language="javascript"><![CDATA[ Xrm.Page.getControl("controlname").getVisible(); $end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
